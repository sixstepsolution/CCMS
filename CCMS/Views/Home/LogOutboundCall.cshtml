                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>

    <div class="modal" id="addDocumentsModal">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">Add Documents</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <fieldset style="margin-bottom: 10px;">

                        <div class="row">
                            <div class="col-12 col-lg-10 mb-1">
                                <label>Doc Type</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fa fa-sort-alpha-down"></i>
                                        </span>
                                    </div>
                                    <select class="form-control  form-control-sm" id="DepartmentID" name="DepartmentID" ng-model="CreateNewUserData.DepartmentID"
                                            ng-class="{error: showMsgs && CreateNewUserForm.DepartmentID.$error.required}" ng-required="capturePanel">
                                        <option selected="selected" value="">--Select Document Type--</option>
                                        <option ng-repeat="d in DepartmentsList" ng-value="d.DepartmentID">{{d.DepartmentName}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12 col-lg-10 mb-1">
                                <label>Upload Documents</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fa fa-upload"></i>
                                        </span>
                                    </div>
                                    <input type="file" id="Attachment" class="form-control  form-control-sm" placeholder="Attachment" name="Attachment" ng-model="Attachment">
                                </div>
                            </div>
                            <div class="col-12 col-lg-2 mb-1" style="margin-top:25px">
                                <button type="button" id="loading-btn" class="btn btn-sm btn-success-custom" ng-click="UploadAttachment()"><i class="fa fa-spin spinner" ng-show="UploadAttachmentLoader"></i>&nbsp;Upload</button>
                            </div>
                        </div>






                        <div class="col-lg-12">
                            <div class="col-lg-2" style="margin-top:20px"></div>

                            <div class="col-lg-10">
                                <label style="font-size :12px;font-weight :normal;font-style :italic;">File size should not be greater than 500 kb</label>
                            </div>
                        </div>


                        <div class="col-lg-12" style="margin: 10px auto;">
                            <div class="row">
                                <div class="col-lg-4" style="margin-top: 10px;">
                                    Uploaded Picture
                                </div>

                                <div class="col-lg-4">
                                    <table class="table table-striped table-bordered table-hover tabel-condensed">
                                        <tbody id="AttachmentsList">
                                            <tr ng-repeat="a in AttachmentFiles">
                                                <td>
                                                    {{a.name}}
                                                </td>
                                                <td style="width: 20px;">
                                                    <a href="" ng-click="RemoveAttachment($index)"><i class="fa fa-trash"></i></a>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>


                        </div>


                    </fieldset>

                    <div class="pull-right">
                        <button type="button" id="loading-btn" class="btn btn-sm btn-success-custom" ng-click="SaveDocs()"><i class="fa fa-save" ng-show="imgLoader_Save"></i>Accept</button>
                        <button type="button" id="loading-btn" class="btn btn-sm btn-danger-custom"><i class="fa fa-close" ng-show="imgLoader_Save"></i>Declain</button>
                    </div>
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>
